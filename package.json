{
  "name": "@zigtech/logger",
  "version": "0.0.0",
  "private": "true",
  "description": "Simple logs API for your daily process.",
  "keywords": [
    "log",
    "logger",
    "console",
    "write",
    "log-level",
    "logLevel"
  ],
  "author": "Zigtech IT Team",
  "license": "ISC",
  "main": "build/index.js",
  "scripts": {
    "lint": "npx eslint ./src/**/*.ts",
    "prettier": "npx prettier ./src/**/*.ts --write",
    "beautify": "npm run prettier && npm run lint",
    "postcheckout": "npm run beautify",
    "postinstall": "npm run beautify",
    "precompile": "rm -rf build",
    "compile": "tsc -p .",
    "postcompile": "npx tsc-alias && rm -rf build/package.json && mv build/src build"
  },
  "devDependencies": {
    "@types/node": "12.19.12",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "7.17.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.7",
    "lerna": "3.22.1",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "tsc-alias": "1.2.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3"
  }
}
